package SeventhHW;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;

class Comparablecourse extends Student implements Comparable<Student> {
	
	@Override
	public int compareTo(Student st) {
		return this.getCourse() - st.getCourse();
	}
}


public class Appl {
	public static void main(String[] args){
		List<Student> list = new ArrayList<>();
		Comparablecourse c = new Comparablecourse();
		list.add(new Student("Ivan",1));
		list.add(new Student("Petro",1));
		list.add(new Student("Oleg",2));
		list.add(new Student("Orest",3));
		list.add(new Student ("Andriy",4));
	
		Student tmp = new Student();
		tmp.printStudents(list, 1);
		
		Iterator it = list.iterator();
		
		list.sort(list);
		
		while(it.hasNext()) {
			System.out.println(it.next());
		}
		
	}
}
